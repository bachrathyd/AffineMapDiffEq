using Profile
using StaticArrays
using DifferentialEquations

# using Memoization

using MDBM
#TODO:  @fastmath @inbounds
#@memoize
function f_now(p, t)
    ζ, δ, ϵ, b, τ , T  = p
   # println("Computed $t")
   # println("Computed $p")
   SA[-(δ+ϵ*     cos(t)), -2*ζ]
   #SA[-(δ+ϵ*sign(cos(t))), -2*ζ]
end
#@memoize 
function f_past(p, t)
    ζ, δ, ϵ, b, τ , T  = p
    SA[b, 0]
end

function DelayMathieu(u, h, p, t)
    ζ, δ, ϵ, b, τ , ν, T = p
    dx = u[2]
    ddx = [-(δ+ϵ*cos(t)), -2*ζ]' * u + b * h(p, t - τ)[1] + ν*h(p, t - τ, Val{1})[2]
    SA[dx, ddx]
end


ζ = 0.03  
δ = 1.1
ϵ = 0.2
τ = 2pi 
b = 0.2
ν=0.25 # Neutral coefficient
T= 6;2pi
p = ζ, δ, ϵ, b, τ , ν, T 
#p = (ζ, ωn, k, τ,10.0)

u0 = SA[1.0, 1.0]
h(p, t) = SA[0.0; -0.0]
h(p, t, deriv::Type{Val{1}})= SA[0.0, 0.0]
probMathieu = DDEProblem(DelayMathieu, u0, h, (0.0, T * 20.0), p; constant_lags=[τ],neutral=true)

sol = solve(probMathieu, MethodOfSteps(BS3()))#abstol,reltol
plot(sol)



plot(sol(sol.t[end] .- (0.0:0.01:τ*1.0)))

Base.:+(a::SVector, b::Bool) = a .+ b


Nstep = 100
τmax = 2pi+0.1
dpdp = dynamic_problemSampled(probMathieu, MethodOfSteps(BS3()), τmax, T; Historyresolution=Nstep, eigN=50, zerofixpont=true);
dpdp = dynamic_problemSampled(probMathieu, MethodOfSteps(BS3()), τmax, T; Historyresolution=Nstep, eigN=5, zerofixpont=true);
dpdp = dynamic_problemSampled(probMathieu, MethodOfSteps(BS3()), τmax, T; Historyresolution=Nstep, eigN=1, zerofixpont=true);

# fix point by affine map
muaff, s0aff = affine(dpdp; p=p);
mus0, As = spectrum(dpdp; p=p);
plot(log.(abs.(muaff[1])))
plot!(log.(abs.(mus0)))


scatter(muaff[1])
scatter!(mus0)
plot!(sin.(0:0.01:2pi),cos.(0:0.01:2pi))





